{
  "name": "nmstate-console-plugin",
  "version": "0.0.1",
  "private": true,
  "repository": "git@github.com:upalatucci/nmstate-console-plugin.git",
  "license": "Apache-2.0",
  "types": "./src/types/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "yarn clean && NODE_ENV=production yarn webpack",
    "dev": "yarn webpack serve",
    "start-console": "./scripts/start-console.sh",
    "create-cluster": "./scripts/deploy-cluster.sh",
    "destroy-cluster": "./scripts/clean-cluster.sh",
    "login-cluster": "oc login https://127.0.0.1:6443 --token abcdef.0123456789abcdef",
    "cypress": "cypress run --browser chrome",
    "i18n": "i18next \"src/**/*.{js,jsx,ts,tsx}\" [-oc] -c i18next-parser.config.mjs",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:updateSnapshot": "jest --updateSnapshot",
    "generate-types": "cd ./src/nmstate-types && ./download-nmstate-crds.sh && cd ../..",
    "helm:lint": "helm lint deployment/nmstate-console-plugin/",
    "helm:build": "mkdir -p tmp && helm package -d tmp deployment/nmstate-console-plugin/"
  },
